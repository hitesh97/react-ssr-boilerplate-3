{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:build-client dev:build-server dev:server",
    "dev:server": "cross-env PORT=3001 nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "clean": "rm -rf node_modules",
    "clean:install": "yarn clean && yarn",
    "build-server": "webpack --config webpack.server.js",
    "build-client": "webpack --config webpack.client.js",
    "build": "cross-env PORT=3001 npm-run-all --parallel build-client build-server",
    "start:prod:server": "cross-env PORT=3001 \"node build/bundle.js\""
  },
  "author": "Hitesh Khatri",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "1.7.3",
    "cross-env": "^5.2.0",
    "express": "4.16.3",
    "express-http-proxy": "1.4.0",
    "lodash": "4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "1.5.0",
    "webpack": "4.20.2",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "webpack-cli": "^3.1.2"
  }
}
